{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es2016",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "outDir": "./publico",
    "baseUrl": "./fuente",
    "resolveJsonModule": true,
  },
  "include": ["./fuente/**/*", "../../tipos/**/*"],
  "exclude": ["node_modules"],
}
